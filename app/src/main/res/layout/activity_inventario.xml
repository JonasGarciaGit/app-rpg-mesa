    <EditText
        android:id="@+id/bonusAtq2"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/nomeAtq3"
