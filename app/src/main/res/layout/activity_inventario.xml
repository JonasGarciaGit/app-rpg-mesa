    <TextView
        android:id="@+id/textView25"
        android:layout_width="67dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="Dano/Tipo"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/nomeAtq2"
